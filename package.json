{
  "name": "CPhilipse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean && yarn",
    "test": "jest",
    "test-update": "jest -u",
    "android": "cd ./android && gradlew app:assembleDebug && gradlew installDebug",
    "run-android": "react-native run-android",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "build-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "keytool": "keytool -list -v -keystore android/app/HvGeAReleaseKey.keystore -alias HvGeA -storepass HvGeA -keypass HvGeA",
    "run-android-prd": "cd ./android && gradlew assembleRelease && cd .. && react-native run-android --variant=release",
    "run-android-prd-bundle": "cd ./android && gradlew bundleRelease && gradlew assembleRelease && cd ../ && react-native run-android --variant=release",
    "run-android-prd-stackoverflow": "cd ./android && gradlew assembleRelease -x bundleReleaseJsAndAssets && cd .. && react-native run-android --variant=release",
    "dev-menu": "adb shell input keyevent 82",
    "fastlane-android": "cd android && bundle exec fastlane android build && cd ..",
    "fastlane-ios": "cd ios && bundle exec fastlane ios build && cd ..",
    "run-ios": "react-native run-ios",
    "run-ios-11": "react-native run-ios --simulator='iPhone11'",
    "pods": "cd ios && pod install && cd ..",
    "clean-pods": "cd ios && pod deintegrate && pod cache clean --all && pod install && cd ..",
    "prettier": "prettier --write --config ./.prettierrc './src/**/*.{ts,tsx}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fix-lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@reduxjs/toolkit": "^1.6.0",
    "react": "17.0.2",
    "react-native": "^0.64.2",
    "react-native-flipper": "^0.94.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.2.0",
    "react-native-redash": "^16.0.11",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-shared-element": "^0.7.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-navigation-shared-element": "^5.0.0-alpha1",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-flipper": "^1.4.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/runtime": "^7.14.6",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.10",
    "@types/react-native-vector-icons": "^6.4.7",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "jest": "^27.0.5",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.3.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
